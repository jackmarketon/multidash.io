{
  "name": "multidash",
  "version": "0.0.1",
  "description": "A tool to allow you to run multiple logging utilities (like webpack watch & karma testing) in the same terminal window, and put your config so it's cross developer friendly.",
  "main": "bin/index.js",
  "scripts": {
    "dev": "DEBUG=* ./bin/index.js -p \"ls -al\" -p \"pwd\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/eslint/bin/eslint.js -c .eslintrc lib/** bin/**"
  },
  "author": {
    "name": "Jackson Marketon",
    "email": "jmarketon@gmail.com",
    "url": "http://github.com/jackmarketon"
  },
  "license": "ISC",
  "keywords": [
    "dashboard",
    "curses"
  ],
  "dependencies": {
    "blessed": "^0.1.81",
    "commander": "^2.9.0",
    "debug": "^2.2.0"
  },
  "engines": {
    "node": "^5.4.0"
  },
  "private": true,
  "devDependencies": {
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.1.2"
  }
}
